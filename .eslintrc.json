{
  "extends": ["next/core-web-vitals", "plugin:import/recommended", "prettier"],
  "rules": {
    "import/order": [
      "error",
      {
        "groups": [["builtin","external"], "internal", ["sibling", "parent"], "index"],
        "pathGroupsExcludedImportTypes": ["internal", "react"],
        "newlines-between": "always"
      }
    ],
    "no-unused-vars": 1
  }
}
